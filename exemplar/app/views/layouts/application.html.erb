  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <%= stylesheet_link_tag    "application", media: "all" %>

  <!--[if lt IE 9]>
  <%= stylesheet_link_tag    "application-fixedwidth.css", media: "all" %>
